<Window x:Class="magazyn_projekt.removeUserPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:magazyn_projekt"
        mc:Ignorable="d"
        Title="removeUserPopup" Height="157" Width="150">
    <Grid Margin="0,0,10,-6">
        <TextBox x:Name="removeidTexbox" HorizontalAlignment="Left" Margin="10,22,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="removeButton" Content="remove user" HorizontalAlignment="Center" Margin="0,54,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.117,0.056" Click="Button_Click"/>
        <Button x:Name="cancelButton" Content="cancel" HorizontalAlignment="Center" Margin="0,94,0,0" VerticalAlignment="Top" Width="80" Click="cancelButton_Click"/>

    </Grid>
</Window>

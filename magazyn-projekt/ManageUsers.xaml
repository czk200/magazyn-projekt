<Window x:Class="magazyn_projekt.ManageUsers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:magazyn_projekt"
        mc:Ignorable="d"
        Title="ManageUsers" Height="766" Width="1366">
    <Grid>
        <Button Content="back" HorizontalAlignment="Left" Margin="27,28,0,0" VerticalAlignment="Top" Name="backButton" Click="backButtonClick" Height="28" Width="484"/>
        <ListBox Background="Gray" Width="800" Height="650" Margin="540,28,26,622" SelectionChanged="ListBox_SelectionChanged" Name="listBox1" ItemsSource="{Binding users}">
            <TextBox Text="{Binding Path=userid}"/>
            <TextBox Text="{Binding Path=status}"/>
        </ListBox>
                 
    </Grid>
</Window>
